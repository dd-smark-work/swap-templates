{
  "name": "@ali/<%- name %>",
  "assets": {
    "type": "command",
    "command": {
      "cmd": [
        "tnpm ii",
        "tnpm run build",

        "mkdir -p $BUILD_DEST/es",
        "mkdir -p $BUILD_DEST/lib",
        "cp -r ./es/* $BUILD_DEST/es",
        "cp -r ./lib/* $BUILD_DEST/lib",

        "mv ./package.json $BUILD_DEST"
      ]
    }
  }
}
